function l_plotpsi(x, n, a)
    
   P = zeros(n, x);
    
    for i = 1:n
        
        P(i,:) = lg
        
    end
end





function psi = l_generatepsi(x, n, a)
    
    
    
    
    L = l_generateL(x, n, a);
    psi = sqrt((gamma(n+1))/gamma(n+a+1)).*L(n,:).*x.^(a/2).*exp(-x/2);
    
    
end
